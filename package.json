{
  "name": "@quizzit/backend",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "tsc": "tsc",
    "clear:typescript": "rm -rf ./api/typescript/src",
    "generate:typescript": "asyncapi generate models typescript ./api/websocket-asyncapi.yaml -o ./api/typescript/src --tsModuleSystem=CJS --tsExportType=named --tsEnumType=enum --tsModelType=interface",
    "regenerate:typescript": "npm run clear:typescript && npm run generate:typescript",
    "module:typescript": "tsc api/typescript/src/index.ts -d --lib esnext --outDir api/typescript/dist",
    "clear:golang": "rm -rf ./internal/generated-sources/dto",
    "generate:golang": "asyncapi generate models golang ./api/websocket-asyncapi.yaml -o ./internal/generated-sources/dto --packageName=quizzit_dto",
    "regenerate:golang": "npm run clear:golang && npm run generate:golang"
  },
  "devDependencies": {
    "@asyncapi/cli": "^0.37.2",
    "@asyncapi/modelina": "^1.3.2",
    "typescript": "^5.0.4"
  },
  "author": "",
  "license": "MIT"
}
